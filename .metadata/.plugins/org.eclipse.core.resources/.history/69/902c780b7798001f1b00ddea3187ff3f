package loop;

public class PrintTraingle {
//	
//	*  i=0 :j=1
//	** i=1 ;j=2
//	***i=2 ;j=3
	public static void main(String[] args) {
		printTriangle(3);

	}

	public static void printTriangle(int rows) {

		for(int i=0 ;i<rows ;i++) {
			for(int j=1 ;i<rows ;i++) {
				
				System.out.println("8");
			}
		}
	}

}
