package array;

import java.util.Arrays;

public class Product_of_Array_Except_Self {

	public static void main(String[] args) {
		int[] nums = { 1, 2, 3, 4 };
		int product = 1;
		
		System.out.println(Arrays.toString(extracted(nums, product)));

	}

	private static int[] extracted(int[] nums, int product) {
		
		for (int p : nums) {
			product *= p;
		}


		
		for (int p : nums) {
			
			return new int[] {product/p};
		}
		return nums;
	}

}
