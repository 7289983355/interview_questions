package string;

public class Reverse_String {

	public static void main(String[] args) {

		String[] charArr = { "h", "e", "l", "l", "o" };
		System.out.println(reverseString(charArr));
	}

	private static String[] reverseString(String[] charArr) {

		int start = 0;
		int end = charArr.length;

		while (start < end) {

			String temp = charArr[start];
			charArr[start] = charArr[charArr.length - 1];
			charArr[charArr.length - 1] = charArr[start];
		}

		return charArr;

	}

}
