package linkedListDemo;

public class LinkedList {

	Node head;

	class Node {
		int data;
		Node next;

		Node(int data) {

			this.data = data;
			this.next = null;
		}
	}

	public void add(int data) {

		Node newnode = new Node(data);

		if (head == null) {
			head = newnode;
		}

		Node current = head;

		while (current.next != null) {

			current = current.next; // ->A->B->C->D->null
		}
		current = newnode;

	}

	public static void main(String[] args) {

	}

}
