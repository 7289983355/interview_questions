package HeapMin;

import java.util.PriorityQueue;

public class MinHeap {

	public static void main(String[] args) {
		// create a minHeap

		PriorityQueue<Integer> minHeap = new PriorityQueue<>();
		minHeap.add(10);
		minHeap.add(1);
		minHeap.add(9);
		minHeap.add(8);
		minHeap.add(7);
		minHeap.add(6);
		minHeap.add(5);
		minHeap.add(3);
		
		

	}

}
