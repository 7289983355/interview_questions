package linkedListDemo;

public class LinkedList {

	Node head;

	class Node {
		int data;
		Node next;

		Node(int data) {

			this.data = data;
			this.next = null;
		}
	}

	public void add(int data) {
		
		Node first= new Node(data);
		
		if(head==null) {
			head.next=first;
			head =first;
			
			
		}

	}

	public static void main(String[] args) {

	}

}
