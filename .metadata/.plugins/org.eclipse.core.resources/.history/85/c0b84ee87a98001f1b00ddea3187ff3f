package loop;

public class Prime_Number_Check {

	public static void main(String[] args) {

		int[] number = { 4, 6, 7, 8, 9, 1, 2, 3 };

		isPrime(number);
	}

	private static void isPrime(int[] number) {

		
		
		for(int num:number){
		for (int i = 2; i < Math.sqrt(num); i++) {
			
			if(num%i==0) {
				
				System.out.println("NOT Prime " +num);
			}

		}
		}

	}

}
